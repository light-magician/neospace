{
  "name": "infinite-canvas-app",
  "version": "1.0.0",
  "description": "An infinite canvas app with dot grid",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-pack": "webpack serve --mode development",
    "build0-pack": "webpack --mode production",
    "build:wasm": "cd wasm && wasm-pack build --target web",
    "build:all": "npm run build:wasm && npm run build"
  },
  "dependencies": {
    "@melloware/coloris": "^0.21.1",
    "fabric": "^5.3.1",
    "gifuct-js": "^2.1.2",
    "marked": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}